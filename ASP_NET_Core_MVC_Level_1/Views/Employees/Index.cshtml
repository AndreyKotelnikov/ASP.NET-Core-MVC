@model System.Collections.Generic.IEnumerable<ASP_NET_Core_MVC_Level_1.ViewModels.EmployeeView>

@{
    ViewBag.Title = "Сотрудники";
}

<h2>@ViewBag.Title</h2>
<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>SurName</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.Name</td>
            <td>@employee.SecondName</td>
        </tr>
    }
    </tbody>
</table>